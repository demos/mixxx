<?xml version='1.0' encoding='utf-8'?>
<MixxxMIDIPreset schemaVersion="1" mixxxVersion="2.1+">
  <info>
    <name>Pioneer CDJ-900</name>
    <author>Jean Claveau based on Ilkka Tuohela's Pioneer CDJ-850 mapping &lt;hile@iki.fi&gt;</author>
    <description>Pioneer CDJ-900 configuration for 2 decks on midi channels 1 and 2</description>
  </info>
  <controller id="PIONEER CDJ-900" port="">
    <scriptfiles>
      <file functionprefix="PioneerCDJ900" filename="Pioneer-CDJ-900-scripts.js"/>
    </scriptfiles>
    <controls>

<!-- Deck 1 -->
  <!-- Left column -->
    <!-- CUES -->
      <control>
        <!--loop_delete-->
        <group>[Channel1]</group>
        <key>PioneerCDJ900.hotcue_last_delete</key>
        <status>0X90</status>
        <midino>0X0D</midino>
        <options> <script-binding/> </options>
      </control>
      <control>
        <!--autobeat_loop_1-->
        <group>[Channel1]</group>
        <key>PioneerCDJ900.hotcue_1_activate</key>
        <status>0X90</status>
        <midino>0X28</midino>
        <options> <script-binding/> </options>
      </control>
      <control>
        <!--hot_cue_a-->
        <group>[Channel1]</group>
        <key>PioneerCDJ900.hotcue_1_activate</key>
        <status>0X90</status>
        <midino>0X18</midino>
        <options> <script-binding/> </options>
      </control>
      <control>
        <!--autobeat_loop_2-->
        <group>[Channel1]</group>
        <key>PioneerCDJ900.hotcue_2_activate</key>
        <status>0X90</status>
        <midino>0X29</midino>
        <options> <script-binding/> </options>
      </control>
      <control>
        <!--hot_cue_b-->
        <group>[Channel1]</group>
        <key>PioneerCDJ900.hotcue_2_activate</key>
        <status>0X90</status>
        <midino>0X19</midino>
        <options> <script-binding/> </options>
      </control>
      <control>
        <!--autobeat_loop_4-->
        <group>[Channel1]</group>
        <key>PioneerCDJ900.hotcue_3_activate</key>
        <status>0X90</status>
        <midino>0X2A</midino>
        <options> <script-binding/> </options>
      </control>
      <control>
        <!--hot_cue_c-->
        <group>[Channel1]</group>
        <key>PioneerCDJ900.hotcue_3_activate</key>
        <status>0X90</status>
        <midino>0X1A</midino>
        <options> <script-binding/> </options>
      </control>
      <control>
        <!--autobeat_loop_8-->
        <group>[Channel1]</group>
        <key>PioneerCDJ900.hotcue_4_activate</key>
        <status>0X90</status>
        <midino>0X2B</midino>
        <options> <script-binding/> </options>
      </control>
      <control>
        <!--hot_cue_call-->
        <group>[Channel1]</group>
        <key>PioneerCDJ900.hotcue_4_activate</key>
        <status>0X90</status>
        <midino>0X1C</midino>
        <options> <script-binding/> </options>
      </control>


    <!-- MAIN CONTROLS -->
      <control>
        <!--reverse-->
        <group>[Channel1]</group>
        <key>PioneerCDJ900.reverse_toggle</key>
        <status>0X90</status>
        <midino>0X21</midino>
        <options> <script-binding/> </options>
      </control>

      <control>
        <!--next_track-->
        <group>[Playlist]</group>
        <key>SelectNextTrack</key>
        <status>0X90</status>
        <midino>0X04</midino>
      </control>
      <control>
        <!--previous_track-->
        <group>[Playlist]</group>
        <key>SelectPrevTrack</key>
        <status>0X90</status>
        <midino>0X05</midino>
      </control>

      <control>
        <!--seek_backward-->
        <group>[Channel1]</group>
        <key>back</key>
        <status>0X90</status>
        <midino>0X03</midino>
      </control>
      <control>
        <!--seek_forward-->
        <group>[Channel1]</group>
        <key>fwd</key>
        <status>0X90</status>
        <midino>0X02</midino>
      </control>

      <control>
        <!--cue-->
        <group>[Channel1]</group>
        <key>cue_default</key>
        <status>0X90</status>
        <midino>0X01</midino>
      </control>

      <control>
        <!--play_pause-->
        <group>[Channel1]</group>
        <key>play</key>
        <status>0X90</status>
        <midino>0X00</midino>
      </control>



<!-- Center Column -->
      <control>
        <!--autocue_time_mode-->
        <group>[Channel1]</group>
        <key>beats_translate_curpos</key>
        <status>0X90</status>
        <midino>0X0E</midino>
      </control>

    <!-- PLAYLIST -->
      <control>
        <!--select_knob-->
        <group>[Playlist]</group>
        <key>PioneerCDJ900.select_turn</key>
        <status>0XB0</status>
        <midino>0X4F</midino>
        <options> <script-binding/> </options>
      </control>
      <control>
        <!--select_knob_push-->
        <group>[Channel1]</group>
        <status>0X90</status>
        <midino>0X33</midino>
        <key>PioneerCDJ900.select_push</key>
        <description></description>
        <options>
          <script-binding/>
        </options>
      </control>
      <control>
        <!--select_back_knob-->
        <group>[Playlist]</group>
        <status>0X90</status>
        <midino>0X32</midino>
        <key>PioneerCDJ900.select_back</key>
        <description></description>
        <options>
          <script-binding/>
        </options>
      </control>

    <!-- LOOPS -->
      <control>
        <!--loop_call_next-->
        <group>[Channel1]</group>
        <key>loop_double</key>
        <status>0X90</status>
        <midino>0X0B</midino>
      </control>
      <control>
        <!--autobeat_select-->
        <group>[Channel1]</group>
        <key>loop_enabled</key>
        <status>0X90</status>
        <midino>0X2D</midino>
      </control>
      <control>
        <!--loop_call_previous-->
        <group>[Channel1]</group>
        <key>loop_halve</key>
        <status>0X90</status>
        <midino>0X0C</midino>
      </control>

      <control>
        <!--loop_in-->
        <group>[Channel1]</group>
        <key>loop_in</key>
        <status>0X90</status>
        <midino>0X06</midino>
      </control>
      <control>
        <!--loop_out-->
        <group>[Channel1]</group>
        <key>loop_out</key>
        <status>0X90</status>
        <midino>0X07</midino>
      </control>
      <control>
        <!--needle_touch-->
        <group>[Channel1]</group>
        <key>loop_scale</key>
        <status>0XB0</status>
        <midino>0X1C</midino>
        <options> <invert/> </options>
      </control>
      <control>
        <!--reloop_exit-->
        <group>[Channel1]</group>
        <key>reloop_exit</key>
        <status>0X90</status>
        <midino>0X08</midino>
      </control>
      <control>
        <!--loop_memory-->
        <group>[Channel1]</group>
        <key>beatsync</key>
        <status>0X90</status>
        <midino>0X0A</midino>
      </control>

      <control>
        <!--slip-->
        <group>[Channel1]</group>
        <key>slip_enabled</key>
        <status>0X90</status>
        <midino>0X2C</midino>
      </control>

    <!-- JOG -->
      <control>
        <!--jog_dial-->
        <group>[Channel1]</group>
        <key>PioneerCDJ900.jog_wheel</key>
        <status>0XB0</status>
        <midino>0X30</midino>
        <options> <script-binding/> </options>
      </control>
      <control>
        <!--jog_scratch-->
        <group>[Channel1]</group>
        <key>PioneerCDJ900.jog_scratch</key>
        <status>0XB0</status>
        <midino>0X10</midino>
        <options> <script-binding/> </options>
      </control>
      <control>
        <!--jog_touch-->
        <group>[Channel1]</group>
        <key>scratch2_enable</key>
        <status>0X90</status>
        <midino>0X20</midino>
      </control>

<!-- Right Column -->
      <control>
        <!--eject-->
        <group>[Channel1]</group>
        <key>eject</key>
        <status>0X90</status>
        <midino>0X2F</midino>
      </control>
      <control>
        <!--touch_brake-->
        <group>[Channel1]</group>
        <key>PioneerCDJ900.vinyl_speed_adjust</key>
        <status>0XB0</status>
        <midino>0X1E</midino>
        <options> <script-binding/> </options>
      </control>

      <control>
        <!--tempo_range-->
        <group>[Channel1]</group>
        <status>0X90</status>
        <midino>0X10</midino>
        <key>PioneerCDJ900.tempo_range</key>
        <description></description>
        <options>
          <script-binding/>
        </options>
      </control>

      <control>
        <!--master_tempo-->
        <group>[Channel1]</group>
        <key>keylock</key>
        <status>0X90</status>
        <midino>0X11</midino>
      </control>
      <control>
        <!--tempo (pitch)-->
        <group>[Channel1]</group>
        <key>rate</key>
        <status>0XB0</status>
        <midino>0X1D</midino>
        <options> <invert/> </options>
      </control>




    </controls>
  </controller>
</MixxxMIDIPreset>
